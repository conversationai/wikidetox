{
  "name": "@conversationai/conversationai-wikidetox-convapi",
  "description": "Web server API to support viewing Wikipedia Talk page conversations using BigQuery.",
  "repository": "https://github.com/conversationai/wikidetox",
  "version": "0.0.1",
  "license": "Apache-2.0",
  "scripts": {
    "setup": "mkdir -p build/server/ && mkdir -p build/config/ && rsync --ignore-existing server_config.template.json build/config/server_config.json",
    "cp-static-web-app": "rsync -a ../conv-viewer-webapp/dist/* ./build/static",
    "build": "yarn run cp-static-web-app && tsc --outdir build/server/",
    "build:watch": "tsc --outdir build/server/ --watch",
    "serve": "node build/server/run_server.js build/config/server_config.json",
    "serve:watch": "yarn run build:watch & NODE_ENV='development' nodemon --inspect build/server/run_server.js build/config/server_config.json",
    "serve:dev": "NODE_ENV='development' node build/server/run_server.js build/config/server_config.json",
    "serve:prod": "NODE_ENV='production' node build/server/run_server.js build/config/server_config.json",
    "test": "mocha build/server/*_test.js build/server/setup/*_test.js",
    "start": "yarn run serve:prod"
  },
  "dependencies": {
    "@conversationai/wpconvlib": "^0.0.2",
    "@google-cloud/debug-agent": "^2.6.0",
    "@google-cloud/logging": "^2.0.0",
    "@google-cloud/spanner": "^1.4.0",
    "@google-cloud/trace-agent": "^3.0.0",
    "arraybuffer-to-string": "^1.0.2",
    "body-parser": "^1.17.1",
    "byline": "^5.0.0",
    "compression": "^1.6.2",
    "concurrently": "^3.4.0",
    "connect-memcached": "^0.2.0",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.4.1",
    "cors": "^2.8.1",
    "express": "^4.17.3",
    "express-enforces-ssl": "^1.1.0",
    "express-recaptcha": "^2.1.0",
    "express-session": "^1.15.1",
    "form-data": "^2.1.2",
    "googleapis": "^39.1.0",
    "gstore-node": "^1.3.1",
    "hash.js": "^1.1.3",
    "helmet": "^3.4.1",
    "line-by-line": "^0.1.5",
    "morgan": "^1.8.1",
    "nodemon": "^1.12.1",
    "passport": "^0.3.2",
    "passport-google-oauth20": "^1.0.0",
    "recaptcha2": "^1.0.8",
    "request": "^2.78.0",
    "seedrandom": "^2.4.3",
    "serve-favicon": "^2.3.0",
    "yargs": "^8.0.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.3",
    "@types/byline": "^4.2.31",
    "@types/chai": "^4.0.4",
    "@types/compression": "^0.0.33",
    "@types/express-session": "^0.0.32",
    "@types/helmet": "^0.0.33",
    "@types/line-by-line": "^0.1.1",
    "@types/mocha": "^2.2.41",
    "@types/passport": "^0.3.4",
    "@types/request": "^2.0.4",
    "@types/seedrandom": "^2.4.27",
    "@types/supertest": "^2.0.0",
    "@types/yargs": "^8.0.2",
    "chai": "^4.1.2",
    "supertest": "^3.0.0",
    "tslint": "^4.5.1",
    "typedoc": "^0.5.7",
    "typescript": "^3.0.1"
  },

  "engines": {
    "node": "8.x"
  }

}
